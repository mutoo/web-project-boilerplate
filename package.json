{
  "name": "web-project-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for web project with webpack + es6 + sass.",
  "dependencies": {
    "axios": "^0.19.0",
    "jquery": "^3.3.1",
    "react": "^16.10.2",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-router": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "^3.0.0",
    "es6-promise": "^4.2.4",
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.16.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.15",
    "sass-loader": "^7.0.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.babel.js --env development --hot --inline",
